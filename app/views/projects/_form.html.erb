<%= form_for project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.fields_for :users, project.users.build do |users_fields| %>
    <%= users_fields.label :email %>
    <%= users_fields.text_field :email %>
  <% end %>

  <%= f.submit %>
<% end %>